{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "space-before-function-paren": "off",
      "camelcase": "off",
      "prefer-const": "off",
      "quotes": "off"
    },
    "ignorePatterns": ["test/**/*.js"]
}
